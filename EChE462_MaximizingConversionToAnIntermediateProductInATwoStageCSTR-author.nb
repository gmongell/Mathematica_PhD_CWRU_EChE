Cell[TextData[{
 "The equations describing the steady state condition of the CSTR are:\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"0", "=", 
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["R", 
         RowBox[{"n", "-", "1"}]], "-", 
        SubscriptBox["R", "n"]}], ")"}], " ", 
      RowBox[{"q", "/", 
       SubscriptBox["V", "n"]}]}], "-", 
     RowBox[{
      SubscriptBox["A", 
       StyleBox["R",
        FontSlant->"Plain"]], " ", 
      SuperscriptBox["e", 
       RowBox[{
        RowBox[{"-", 
         SubscriptBox["E", 
          StyleBox["R",
           FontSlant->"Plain"]]}], "/", 
        SubscriptBox["T", "n"]}]], 
      SubscriptBox["R", "n"]}]}]}], TraditionalForm]], "InlineMath"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{"0", "=", 
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["P", 
         RowBox[{"n", "-", "1"}]], "-", 
        SubscriptBox["P", "n"]}], ")"}], 
      RowBox[{"q", "/", 
       SubscriptBox["V", "n"]}]}], "+", 
     RowBox[{
      SubscriptBox["A", 
       RowBox[{
        StyleBox["R",
         FontSlant->"Plain"], " "}]], " ", 
      SuperscriptBox["e", 
       RowBox[{
        RowBox[{"-", 
         SubscriptBox["E", 
          StyleBox["R",
           FontSlant->"Plain"]]}], "/", 
        SubscriptBox["T", "n"]}]], " ", 
      SubscriptBox["R", "n"]}], "-", 
     RowBox[{
      SubscriptBox["A", 
       StyleBox["P",
        FontSlant->"Plain"]], " ", 
      SuperscriptBox["e", 
       RowBox[{
        RowBox[{"-", 
         SubscriptBox["E", "P"]}], "/", 
        SubscriptBox["T", "n"]}]], 
      SubscriptBox["P", "n"]}]}]}], TraditionalForm]], "InlineMath"],
 ",\nwhere ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["R", "n"], TraditionalForm]], "InlineMath"],
 " and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["P", "n"], TraditionalForm]], "InlineMath"],
 " are the concentrations, ",
 Cell[BoxData[
  FormBox[
   RowBox[{"q", " "}], TraditionalForm]], "InlineMath"],
 "is the flow rate, and ",
